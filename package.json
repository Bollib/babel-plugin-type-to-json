        "type",
        "json",
        "babel",
        "transformer"
    ],
    "author": "Tar van Krieken",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/LaunchMenu/babel-plugin-type-to-json/issues"
    },
    "homepage": "https://github.com/LaunchMenu/babel-plugin-type-to-json#readme",
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/plugin-proposal-class-properties": "^7.3.0",
