        "@babel/runtime": "^7.3.1"
    },
    "dependencies": {
        "@babel/core": "^7.2.2",
        "@babel/helper-create-class-features-plugin": "^7.3.0",
        "@babel/helper-plugin-utils": "^7.0.0",
